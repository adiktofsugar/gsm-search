{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "node": true
  },
  "globals": {
    "Promise": true
  },
  "extends": [
    "eslint:recommended"
  ],

  "rules": {
    "strict": 0,
    "no-unused-vars": 0,
    "curly": [1, "multi-line"],
    "no-bitwise": 1,
    "no-console": 0,
    "no-debugger": 1
  },

  "ecmaFeatures": {
  }
}
